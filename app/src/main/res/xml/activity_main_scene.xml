<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/appName"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="215dp"
            android:layout_height="58dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="120dp" />
        <Constraint
            motion:layout_constraintVertical_bias="0.34"
            android:layout_height="45dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="170dp"
            motion:layout_constraintBottom_toTopOf="@+id/des_dev"
            motion:layout_constraintHorizontal_bias="0.496"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="278dp"
            motion:layout_constraintTop_toBottomOf="@+id/appName"
            android:id="@+id/welc" />
        <Constraint
            motion:layout_editor_absoluteY="426dp"
            android:layout_height="45dp"
            motion:layout_constraintHorizontal_bias="0.497"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="168dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/des_dev" />
        <Constraint
            motion:layout_constraintVertical_bias="0.454"
            android:layout_height="34dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="8dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.498"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="140dp"
            motion:layout_constraintTop_toBottomOf="@+id/ind"
            android:id="@+id/dev" />
        <Constraint
            android:layout_height="42dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="157dp"
            android:id="@+id/ind"
            motion:layout_constraintTop_toBottomOf="@+id/des_dev" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/ind"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="157dp"
            android:layout_height="42dp"
            motion:layout_constraintTop_toBottomOf="@+id/des_dev"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/dev"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="140dp"
            android:layout_height="34dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="8dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.018"
            motion:layout_constraintTop_toBottomOf="@+id/ind"
            motion:layout_constraintHorizontal_bias="0.498" />
        <Constraint
            android:id="@+id/welc"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="278dp"
            android:layout_height="45dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="170dp"
            motion:layout_constraintBottom_toTopOf="@+id/des_dev"
            motion:layout_constraintVertical_bias="0.34"
            motion:layout_constraintTop_toBottomOf="@+id/appName"
            motion:layout_constraintHorizontal_bias="0.496" />
        <Constraint
            android:id="@+id/des_dev"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="168dp"
            android:layout_height="45dp"
            motion:layout_constraintTop_toBottomOf="@id/welc"
            motion:layout_constraintHorizontal_bias="0.497"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="216dp" />
        <Constraint
            android:id="@+id/appName"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="215dp"
            android:layout_height="58dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="148dp" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/appName" >
        <Constraint
            android:id="@+id/appName"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="215dp"
            android:layout_height="58dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="148dp" />
        <Constraint
            android:id="@+id/welc"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="278dp"
            android:layout_height="45dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="170dp"
            motion:layout_constraintBottom_toTopOf="@+id/des_dev"
            motion:layout_constraintVertical_bias="0.34"
            motion:layout_constraintTop_toBottomOf="@+id/appName"
            motion:layout_constraintHorizontal_bias="0.496" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/start"
        motion:constraintSetEnd="@+id/appName"
        motion:autoTransition="animateToStart" />
    <ConstraintSet android:id="@+id/Welcome" >
        <Constraint
            android:id="@+id/welc"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="278dp"
            android:layout_height="45dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="170dp"
            motion:layout_constraintBottom_toTopOf="@+id/des_dev"
            motion:layout_constraintVertical_bias="0.34"
            motion:layout_constraintTop_toBottomOf="@+id/appName"
            motion:layout_constraintHorizontal_bias="0.496" />
        <Constraint
            android:id="@+id/appName"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="215dp"
            android:layout_height="58dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="148dp" />
        <Constraint
            android:id="@+id/des_dev"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="168dp"
            android:layout_height="45dp"
            motion:layout_constraintTop_toBottomOf="@id/welc"
            motion:layout_constraintHorizontal_bias="0.497"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="216dp" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/dev_des" >
        <Constraint
            android:id="@+id/appName"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="215dp"
            android:layout_height="58dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="148dp" />
        <Constraint
            android:id="@+id/welc"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="278dp"
            android:layout_height="45dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="170dp"
            motion:layout_constraintBottom_toTopOf="@+id/des_dev"
            motion:layout_constraintVertical_bias="0.34"
            motion:layout_constraintTop_toBottomOf="@+id/appName"
            motion:layout_constraintHorizontal_bias="0.496" />
        <Constraint
            android:id="@+id/des_dev"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="168dp"
            android:layout_height="45dp"
            motion:layout_constraintTop_toBottomOf="@id/welc"
            motion:layout_constraintHorizontal_bias="0.497"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="216dp" />
        <Constraint
            android:id="@+id/ind"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="157dp"
            android:layout_height="42dp"
            motion:layout_constraintTop_toBottomOf="@+id/des_dev"
            motion:layout_constraintStart_toStartOf="parent" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/appName"
        motion:constraintSetEnd="@+id/Welcome"
        motion:autoTransition="animateToStart" />
    <Transition
        motion:constraintSetStart="@+id/Welcome"
        motion:constraintSetEnd="@+id/dev_des"
        motion:autoTransition="animateToStart" />
    <Transition
        motion:constraintSetStart="@+id/dev_des"
        motion:constraintSetEnd="@+id/end"
        motion:autoTransition="animateToEnd" />
</MotionScene>